{
  "authorization": {
    "oauth_api": {
      "credentials": {
        "#data": "%env(string:OAUTH_DATA)%",
        "#appSecret": "%env(string:OAUTH_APP_SECRET)%",
        "appKey": "%env(string:OAUTH_APP_ID)%"
      }
    }
  },
  "parameters": {
    "name": "ConfigRow Name",
    "subscriptionId": "%env(string:TEST_SUBSCRIPTION_ID)%",
    "export": {
      "type": "Usage",
      "aggregation": "UsageQuantity",
      "granularity": "None",
      "timeDimension": {
        "timeFrame": "TheLastMonth"
      },
      "groupingDimensions": [
        "ServiceName",
        "Meter"
      ]
    }
  }
}
