{
  "authorization": {
    "oauth_api": {
      "credentials": {
        "#data": "%env(string:OAUTH_DATA)%",
        "#appSecret": "%env(string:OAUTH_APP_SECRET)%",
        "appKey": "%env(string:OAUTH_APP_ID)%"
      }
    }
  },
  "parameters": {
    "name": "config-row-name",
    "subscriptionId": "%env(string:TEST_SUBSCRIPTION_ID)%",
    "export": {
      "type": "ActualCost",
      "granularity": "Daily",
      "timeDimension": {
        "timeFrame": "WeekToDate"
      },
      "groupingDimensions": [
        "ServiceName"
      ]
    }
  }
}
